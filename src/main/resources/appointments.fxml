<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rs.controller.AppointmentsController">
   <children>
      <DatePicker fx:id="registrationDatePicker" layoutX="191.0" layoutY="166.0" onMouseClicked="#onDatePickerClicked" promptText="Select registration date" />
      <ComboBox fx:id="visitTimeCombo" layoutX="191.0" layoutY="240.0" onMouseClicked="#onTimesComboClicked" prefHeight="31.0" prefWidth="218.0" promptText="Chose visit time" />
      <Button fx:id="registerBtn" layoutX="191.0" layoutY="309.0" mnemonicParsing="false" onAction="#doRegistration" prefHeight="31.0" prefWidth="218.0" text="Register" />
      <Text fx:id="txtTest" layoutX="191.0" layoutY="161.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Visit Date" />
      <Text fx:id="txtTest1" layoutX="191.0" layoutY="235.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Visit Time" />
   </children>
</AnchorPane>
